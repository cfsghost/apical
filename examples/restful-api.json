					"code": 200,
					"success": true,
					"format": {
						"videos": {
							"type": "Array",
							"desc": "List of videos",
							"item": {
								"type": "Object",
								"format": {
									"id": {
										"type": "String",
										"desc": "Video unique ID"
									},
									"name": {
										"type": "String",
										"desc": "Video Name"
									},
									"desc": {
										"type": "String",
										"desc": "Video description"
									}
								}
							}
						}
					},
					"example": [
						{
							"id": "23TplPdS",
							"name": "Pen Pineapple Apple Pen",
							"desc": "What the fuck?"
						},
						{
							"id": "56SDflPzX",
