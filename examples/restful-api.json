		{
			"name": "Get Video List",
			"desc": "This endpoint retrieves all videos.",
			"group": "Video",
			"api": "/apis/videos",
			"method": "GET",
			"params": {
				"query": {
					"limit": {
						"type": "Number",
						"desc": "Limit the number of results returned.",
						"default": 100,
						"required": false
					},
					"offset": {
